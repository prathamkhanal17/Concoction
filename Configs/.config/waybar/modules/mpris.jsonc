{
    "mpris": {
        "format": "{player_icon} {title}",
        "format-paused": "{status_icon} <i>{title}</i>",
        "player-icons": {
            "default": "▶",
            "mpv": "🎵"
        },
        "status-icons": {
            "paused": "⏸"
        },
        "max-length": 20,
        "on-click-prev": "playerctl previous",
        "on-click-play": "playerctl play-pause",
        "on-click-next": "playerctl next"
    }
}
